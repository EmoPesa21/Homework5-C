#include <stdio.h>
int main()
{
	char s[10] = {'w','#','!','%','2','A','u','5','8','f'};
		int i;
		int countN = 0;
			for(i=0;i<10;i++)
				{
					
					if(s[i] == 65) // Other variant is '0' and '9'
						{
							countN += 1;
							printf("Position in masive is:%d\n", i+1);
						}
				}
return 0;
}
/* On which position is the letter 'A'
 OTPUT TERMINAL:
 Position in masive is:6
 */
